  "source": "pragma solidity ^0.5.0;\n\ncontract Todos {\n    bytes32[] todos;\n\n    function addTodo(bytes32 todo) public {\n        todos.push(todo);\n    }\n\n    function getTodos() public view returns (bytes32[] memory) {\n        return todos;\n    }\n}",
